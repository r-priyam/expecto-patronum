{
	"name": "expecto-patronum",
	"version": "1.0.0",
	"private": true,
	"license": "MPL-2.0",
	"author": "r-priyam <roy_priyam@outlook.com>",
	"type": "module",
	"main": "dist/ExpectoPatronum.js",
	"scripts": {
		"build": "rimraf dist && tsc -b src",
		"commands:sync": "discord-commands-sync",
		"commit": "git-cz",
		"dev": "rimraf dist && yarn build && yarn start",
		"dev:watch": "tsc-watch -b src --onSuccess \"yarn start\"",
		"format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
		"lint": "eslint src --ext ts --fix",
		"prepare": "husky install",
		"release": "cliff-jumper",
		"release:test": "cliff-jumper --dry-run",
		"start": "node --enable-source-maps dist/ExpectoPatronum.js",
		"upgrade": "yarn upgrade-interactive",
		"watch": "tsc -b src -w"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,ts}": "eslint --fix --ext mjs,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"resolutions": {
		"minimist": "^1.2.8"
	},
	"dependencies": {
		"@discordjs/builders": "^0.13.0",
		"@mikro-orm/sql-highlighter": "^1.0.1",
		"@sapphire/decorators": "^4.3.8",
		"@sapphire/discord-utilities": "^2.12.0",
		"@sapphire/discord.js-utilities": "4.12.0",
		"@sapphire/framework": "next",
		"@sapphire/plugin-hmr": "^1.2.2",
		"@sapphire/plugin-i18next": "^2.5.4",
		"@sapphire/plugin-logger": "^2.2.3",
		"@sapphire/plugin-scheduled-tasks": "2.3.5",
		"@sapphire/plugin-subcommands": "^2.2.2",
		"@sapphire/stopwatch": "^1.5.2",
		"@sapphire/time-utilities": "^1.7.12",
		"@sapphire/type": "^2.5.1",
		"@sapphire/utilities": "^3.17.0",
		"bull": "^4.16.3",
		"colorette": "^2.0.20",
		"discord.js": "^13.17.1",
		"dotenv": "^16.4.5",
		"figlet": "^1.8.0",
		"gradient-string": "^2.0.2",
		"postgres": "^3.4.4",
		"reflect-metadata": "^0.2.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.8.1",
		"@commitlint/config-conventional": "^17.8.1",
		"@favware/cliff-jumper": "^1.10.0",
		"@sapphire/eslint-config": "^4.4.3",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@types/bull": "^3.15.9",
		"@types/figlet": "^1.7.0",
		"@types/gradient-string": "^1.1.6",
		"@types/node": "^17.0.45",
		"@types/ws": "^8.5.12",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "3.3.0",
		"discord-api-types": "^0.30.0",
		"discord-commands-sync": "^1.2.4",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unicorn": "^43.0.2",
		"husky": "^8.0.3",
		"lint-staged": "^13.3.0",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.3.1",
		"rimraf": "^3.0.2",
		"tsc-watch": "^5.0.3",
		"typescript": "^4.7.2"
	},
	"imports": {
		"#root/*": "./dist/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#utils/*": "./dist/lib/utils/*.js",
		"#classes/*": "./dist/lib/classes/*.js"
	},
	"packageManager": "yarn@3.8.6"
}
